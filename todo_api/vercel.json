{
     "builds": [
          {
               "src": "todo_api/wsgi.py",
               "use": "@vercel/python",
               "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
          }
     ],
     "routes": [
          {
               "src": "/(.*)",
               "dest": "/todo_api/wsgi.py"
          }
     ]
}
